for i in *_Q15_1P.fq.gz; do j=`echo $i | cut -f 1-2 -d _`; gunzip -c ${j}_* | gzip > $j.fq.gz; done
for i in *_Q20_1P.fq.gz; do j=`echo $i | cut -f 1-2 -d _`; gunzip -c ${j}_* | gzip > $j.fq.gz; done



for i in Q10 Q15 Q20; do 
gunzip -c *_${i}_[12][PU].fq.gz | gzip > ${i}.fq.gz &
done


